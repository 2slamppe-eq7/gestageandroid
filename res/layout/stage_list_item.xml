<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/LinearLayout1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/tvNumStage"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
    
        android:textSize="30dp" />

    <TextView
        android:id="@+id/tvEtudiantNom"
        android:layout_width="250dp"
        android:layout_height="wrap_content"

        android:textSize="30dp" />
    <TextView
        android:id="@+id/tvEtudiantPrenom"
        android:layout_width="250dp"
        android:layout_height="wrap_content"

        android:textSize="30dp" />
    <TextView
        android:id="@+id/tvOrganisation"
        android:layout_width="250dp"
        android:layout_height="wrap_content"

        android:textSize="30dp" />
    <TextView
        android:id="@+id/tvMaitreStage"
        android:layout_width="250dp"
        android:layout_height="wrap_content"

        android:textSize="30dp" />
    <TextView
        android:id="@+id/tvDateDebut"
        android:layout_width="250dp"
        android:layout_height="wrap_content"

        android:textSize="30dp" />
    <TextView
        android:id="@+id/tvDateFin"
        android:layout_width="250dp"
        android:layout_height="wrap_content"

        android:textSize="30dp" />
    <TextView
        android:id="@+id/tvDateVisite"
        android:layout_width="250dp"
        android:layout_height="wrap_content"

        android:textSize="30dp" />
    <TextView
        android:id="@+id/tvVille"
        android:layout_width="250dp"
        android:layout_height="wrap_content"

        android:textSize="30dp" />
    
    


</LinearLayout>